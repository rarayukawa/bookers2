<%= form_with model:book, local:true do |f| %>
            <p><%= f.label :title, "Title" %><br>
            <%= f.text_field :title %></p>
            <p><%= f.label :body, "Opinion" %><br>
            <%= f.text_area :body %></p>

            <%= f.submit 'Create Book', class: "btn btn-success" %>
            <% end %>